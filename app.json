{
  "expo": {
    "name": "UTapp",
    "slug": "ukshathi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LeafLogo1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/LeafLogo1024.png",
          "color": "#809c13", 
          "androidMode": "default", 
          "androidCollapsedTitle": "New watering reminder",
          "iosAllowsForegroundNotifications": true
        }
      ]
    ]

    },
    "android": {
      "permissions": ["NOTIFICATIONS"],
      
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LeafLogo1024.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/LeafLogo1024.png"
      
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LeafLogo1024.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "API_BASE_URL": "http://192.168.1.28:5000",
      "API_KEY": "b0426aacb17e876e66782a808069b5fa",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ed1c7715-aabf-4894-b0ce-ab956899905a"
      }
    },
    "owner": "niharapadil"
  }
}

